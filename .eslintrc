{
	"parser": "babel-eslint",
	"extends": ["prettier", "prettier/react"],
	"plugins": ["prettier", "react", "react-hooks"],
	"env": {
		"jest": true,
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 10,
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "error"
	}
}
